{
  "$schema": "node_modules/wrangler/config-schema.json",
  "main": "dist/_worker.js/index.js",
  "name": "astro",
  "compatibility_date": "2025-03-25",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "d1_databases": [
    {
      "binding": "finestdb",
      "database_name": "finestdb",
      "database_id": "a409d3f8-498d-4a22-9da6-121b113e638e",
      "migrations_dir": "drizzle"
    }
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "finestnotes.com",
      "custom_domain": true
    }
  ]
}
