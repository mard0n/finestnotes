---
import { parseResponse } from "hono/client";
import Layout from "../layouts/Layout.astro";
import { client } from "../utils/types";

const res = await client.api.notes.$get();
const notes = await parseResponse(res);
---

<Layout>
  {
    notes.map((note) => (
      <div>
        <h2>{note.title}</h2>
        <p>{note.content}</p>
      </div>
    ))
  }
</Layout>
